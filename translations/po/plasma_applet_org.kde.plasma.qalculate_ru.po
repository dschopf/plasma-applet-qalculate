msgid "Angle unit"
msgstr "Единица измерения углов"

#: ../plasmoid/contents/ui/config/Input.qml:222
msgctxt "Angle Unit"
msgid "None"
msgstr "Нет"

#: ../plasmoid/contents/ui/config/Input.qml:223
msgctxt "Angle Unit"
msgid "Radians"
msgstr "Радианы"

#: ../plasmoid/contents/ui/config/Input.qml:224
msgctxt "Angle Unit"
msgid "Degrees"
msgstr "Градусы"

#: ../plasmoid/contents/ui/config/Input.qml:225
msgctxt "Angle Unit"
msgid "Gradians"
msgstr "Грады"

